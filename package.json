{
  "name": "genshin-impact-auto-track",
  "version": "0.0.1",
  "description": "Node.js N-API binding of GenshinImpact AutoTrack DLL.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "files": [
    "binding.gyp",
    "download-dll.ps1",
    "lib/*.js",
    "lib/*.d.ts",
    "src/"
  ],
  "binary": {
    "module_name": "genshin-impact-auto-track",
    "module_path": "./lib/binding/napi-v{napi_build_version}-{platform}-{libc}-{arch}",
    "remote_path": "v{version}",
    "package_name": "napi-v{napi_build_version}-{platform}-{libc}-{arch}.tar.gz",
    "host": "https://github.com/pboymt/genshin-impact-auto-track/releases/download/",
    "napi_versions": [
      8
    ]
  },
  "scripts": {
    "prebuild": "pwsh download-dll.ps1",
    "install": "node-pre-gyp install --fallback-to-build",
    "test": "node ./test",
    "build": "npm run prebuild && node-pre-gyp build",
    "build:debug": "npm run prebuild && node-pre-gyp build --debug",
    "rebuild": "node-pre-gyp rebuild",
    "pack": "node-pre-gyp package"
  },
  "keywords": [
    "genshin",
    "impact"
  ],
  "author": "pboymt",
  "license": "MIT",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10",
    "node-addon-api": "*"
  },
  "devDependencies": {
    "node-pre-gyp-github": "1.4.4"
  },
  "peerDependencies": {
    "node-gyp": "8.x"
  },
  "peerDependenciesMeta": {
    "node-gyp": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "node-gyp": "8.x"
  },
  "gypfile": true
}